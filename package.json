{
    "name": "nayn.wordpress",
    "version": "1.0.0",
    "description": "NAYN.CO Wordpress Theme",
    "main": "",
    "dependencies": {
        "node-sass": "4.14.1",
        "uglify-js": "3.6.0"
    },
    "scripts": {
        "dev-minifyJS": "uglifyjs dev/js/main.js --output assets/js/main.min.js",
        "prod-minifyJS": "uglifyjs dev/js/main.js --compress --mangle --warn --output assets/js/main.min.js",
        "dev-scss": "node-sass --watch dev/sass -o assets/css",
        "prod-scss": "node-sass dev/sass -o assets/css"
    },
    "author": "hi@nayn.org",
    "license": "AGPL-3.0"
}